<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

	<suite name="Shop Cart Test automation regression suite"
	thread-count="3" parallel="tests" verbose="3">

	<listeners>
		<listener
			class-name="com.qa.opencart.testlisteners.ExtentReportListener" />
	</listeners>

	<test name="Login page Test">
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Account Page Test ">
		<classes>
			<class name="com.qa.opencart.tests.AccountPageTest" />
		</classes>
	</test>

</suite>